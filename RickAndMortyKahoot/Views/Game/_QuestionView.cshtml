@using RickAndMortyKahoot.Models.Questions
@model GameQuestion

<section class="question">
  <header>
    <h1>@Model.Title</h1>
  </header>
  <ul class="choices">
    @for (int i = 0; i < Model.Choices.Count; i++)
    {
      <li id="@(i)">
        @Model.Choices[i]
      </li>
    }
  </ul>
</section>